{
  "name": "djunehor/laravel-sms",
  "description": "Send SMS from your laravel application",
  "keywords": [
    "sms",
    "laravel-package",
    "laravel",
    "php",
    "djunehor"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Zacchaeus Bolaji",
      "email": "djunehor@gmail.com"
    }
  ],
  "minimum-stability": "dev",
    "require": {
        "php": "~7.0|~7.2",
    "guzzlehttp/guzzle": "^6.5@dev"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "3.8.*|4.*",
        "phpunit/phpunit": "^8.0",
        "illuminate/contracts": "^5.8.15|^6.0",
        "illuminate/support": "^5.8.15|^6.0"

    },
  "autoload": {
    "psr-4": {
      "Djunehor\\Sms\\": "src/"
    },
      "files": [
          "src/Helpers/helpers.php"
      ]
  },
  "autoload-dev": {
    "psr-4": {
      "Djunehor\\Sms\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit tests"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Djunehor\\Sms\\SmsServiceProvider"
      ]
    }
  }
}
